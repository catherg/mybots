<robot name = "robot">
    <link name="Torso">
        <inertial>
            <origin xyz="0 0 1.0" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 1.0" rpy="0 0 0"/>
            <geometry>
                <box size="0 1 2" />
            </geometry>
            <material name="Green">
                <color rgba="0 180.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 1.0" rpy="0 0 0"/>
            <geometry>
                <box size="0 1 2" />
            </geometry>
        </collision>
    </link>
    <link name="Cubez0">
        <inertial>
            <origin xyz="0 0 0.5" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 0.5" rpy="0 0 0"/>
            <geometry>
                <box size="2 3 1" />
            </geometry>
            <material name="Green">
                <color rgba="0 180.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.5" rpy="0 0 0"/>
            <geometry>
                <box size="2 3 1" />
            </geometry>
        </collision>
    </link>
    <link name="Cubex1">
        <inertial>
            <origin xyz="0.5 0 0" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0.5 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="1 3 3" />
            </geometry>
            <material name="Blue">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.5 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="1 3 3" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_Cubez0" type="revolute">
       <parent link="Torso"/>
       <child  link="Cubez0"/>
       <origin rpy="0 0 0" xyz="0 0 2.0" />
     <axis xyz="1 0 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <joint name="Cubez0_Cubex0" type="revolute">
       <parent link="Cubez0"/>
       <child  link="Cubex0"/>
       <origin rpy="0 0 0" xyz="1.0 0 2.0" />
     <axis xyz="1 0 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
</robot>